{
  "name": "ang-jsoneditor",
  "version": "2.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mariohmol/ang-jsoneditor"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run build:libcopy && ng build --project packages && rimraf src/ang-jsoneditor",
    "build:lib": "rimraf dist && ng-packagr -p ang-jsoneditor/package.json",
    "build:libcopy": "cp -R dist/ang-jsoneditor src/ang-jsoneditor",
    "watch": "onchange 'ang-jsoneditor/**/*.ts' -- npm run build:lib && npm run build:libcopy",
    "build:full": "npm run build:lib && npm run build:app",
    "publishnpm": "cp README.md ang-jsoneditor/README.md && npm run build:lib && cd dist/ang-jsoneditor && npm publish",
    "unpublish": "npm deprecate -f 'ang-jsoneditor@1.9.3' 'this package has been deprecated'",
    "test": "npm run test:dev && npm run e2e",
    "test:dev": "ng test dev --watch=false",
    "test:unit": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ports": "lsof -i tcp:4201",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i",
    "rebuildsass": "npm rebuild node-sass"
  },
  "dependencies": {
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/http": "*",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "core-js": "^3.6.4",
    "jsoneditor": "^9.1.8",
    "rxjs": "*",
    "webpack": "^4.46.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-devkit/schematics": "^13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.9",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "cpr": "^3.0.0",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine": "^3.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.29.0",
    "tslib": "^2.0.0",
    "typescript": "4.6.4",
    "webpack-cli": "^4.9.2"
  }
}